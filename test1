class Solution {
public:
    void merge(vector<int>& nums1, int m, vector<int>& nums2, int n) {
        for(int i=0;i<n;i++){
            nums1[m+i]=nums2[i];
        }
        int length=m+n;
        for(int i=0;i<(length-1);i++){
            for(int j=0;j<length-i-1;j++){
                if(nums1[j]>nums1[j+1]){
                    int t=nums1[j];
                    nums1[j]=nums1[j+1];
                    nums1[j+1]=t;
                }
            }
        }

        for(int i=0;i<length;i++){
            cout<<nums1[i]<<"  ";
        }
    }
};
